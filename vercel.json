{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/rooms",
      "destination": "/api/socket/server.ts"
    },
    {
      "source": "/api/socket",
      "destination": "/api/socket/server.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "env": {
    "PORT": "3000"
  }
}
