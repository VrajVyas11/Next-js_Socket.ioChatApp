{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/socket/(.*)",
      "destination": "src/app/api/socket/server.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "src/app/api/socket/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "src/**/*"
        ]
      }
    }
  ]
}
