{
  "version": 2,
  "builds": [
    {
      "src": "src/app/api/socket/server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/api/rooms",
      "destination": "/api/socket/server.ts"
    },
    {
      "source": "/api/socket",
      "destination": "/api/socket/server.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "env": {
    "PORT": "3000"
  }
}
