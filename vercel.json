{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "src/app/api/socket/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**/*"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/socket",
      "dest": "/src/app/api/socket/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
