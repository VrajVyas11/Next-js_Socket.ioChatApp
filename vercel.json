{
  "version": 2,
  "routes": [
    {
      "src": "/api/socket",
      "dest": "src/app/api/socket/server.ts"
    },
    {
      "src": "/next.config.js",
      "dest": "_next/next.config.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "src/app/api/socket/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "src/**/*"
        ]
      }
    }
  ]
}
